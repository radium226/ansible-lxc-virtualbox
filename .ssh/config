Host sun.virtualbox
	HostName 127.0.0.1
	User ansible
	Port 2222

Host mercury.lxc
	ProxyCommand ssh -F ./.ssh/config -W %h:%p ansible@sun.virtualbox
	UpdateHostKeys yes
	StrictHostKeyChecking no
	IdentityFile ./.ssh/proxy-id_rsa
